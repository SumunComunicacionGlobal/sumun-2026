{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/button": {
				"border": {
					"radius": true
				}
			},
			"core/group": {
				"color": {
					"background": true
				}
			}
		},
		"border": {
			"color": true,
			"style": true,
			"width": true,
			"radiusSizes": [
				{
					"name": "Border radius 025 (4px)",
					"size": "4px",
					"slug": "025"
				},
				{
					"name": "Border radius 05 (8px)",
					"size": "8px",
					"slug": "05"
				},
				{
					"name": "Border radius 1 (16px)",
					"size": "16px",
					"slug": "1"
				},
				{
					"name": "Border radius 2 (24px)",
					"size": "24px",
					"slug": "1-5"
				},
				{
					"name": "Border radius 3 (32px)",
					"size": "32px",
					"slug": "2"
				},
				{
					"name": "Border radius rounded (999px)",
					"size": "999px",
					"slug": "rounded"
				}
			]
		},
		"color": {
			"custom": true,
			"customDuotone": false,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [],
			"link": true,
			"palette": [
				{
					"name": "Text medium",
					"slug": "text-medium",
					"color": "#6c757d"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#0020c1"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#ffd600"
				},
				{
					"name": "White",
					"slug": "neutral-white",
					"color": "#ffffff"
				},
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				}, 
				{
					"name": "Light",
					"slug": "light",
					"color": "#fbe7ee"
				},
				{
					"name": "Neutral 30",
					"slug": "neutral-30",
					"color": "#DEE2E6"
				},
				{
					"name": "Neutral 60",
					"slug": "neutral-60",
					"color": "#6C757D"
				},
				{
					"name": "Neutral 80",
					"slug": "neutral-80",
					"color": "#343a40"
				},
				{
					"name": "Neutral 90",
					"slug": "neutral-90",
					"color": "#212529"
				},
				{
					"name": "Neutral 100",
					"slug": "neutral-100",
					"color": "#1a1a1a"
				},
				{
					"name": "Foreground",
					"slug": "foreground",
					"color": "#ffffff"
				}
			]
		},
		"custom": {
			"border-radius": {
				"0": "0",
				"025": "4px",
				"05": "8px",
				"1": "16px",
				"1-5": "24px",
				"2": "32px",
				"rounded": "999px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"layout": {
				"gap": "2rem",
				"gap-small": "16px",
				"gap-large": "40px",
				"gap-x-large": "80px",
				"content": "1216px",
				"content-wide": "1440px",
				"padding": "16px"
			},
			"line-height": {
				"headings": 1.2,
				"normal": 1.5,
				"tiny": 1
			}
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--content-wide)"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Space 0.5 (0.5rem - 8px)",
					"size": "0.5rem",
					"slug": "05"
				},
				{
					"name": "Space 1 (1rem - 16px)",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "Space 2 (1.5rem - 24px)",
					"size": "1.5rem",
					"slug": "20"
				},
				{
					"name": "Space 3 (2rem - 32px)",
					"size": "2rem",
					"slug": "30"
				},
				{
					"name": "Space 4 (2.5rem - 40px)",
					"size": "2.5rem",
					"slug": "40"
				},
				{
					"name": "Space 6 (4rem - 64px)",
					"size": "4rem",
					"slug": "60"
				},
				{
					"name": "Space 7 (4.5rem - 72px)",
					"size": "4.5rem",
					"slug": "70"
				},
				{
					"name": "Space 8 (6rem - 96px)",
					"size": "6rem",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "BW Nista",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/BwNistaInt-Md.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "BW Nista",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/BwNistaInt-Lt.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "BW Nista",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "bold",
							"src": [
								"file:./assets/fonts/BwNistaInt-xBd.woff2"
							]
						}
					],
					"fontFamily": "\"BW Nista\", sans-serif",
					"name": "Font Main",
					"slug": "font-main"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Body",
					"size": "1.125rem",
					"slug": "body"
				},
				{
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"name": "H3",
					"size": "2rem",
					"slug": "heading-3"
				},
				{
					"name": "H2",
					"size": "clamp(2rem, 4.4vw, 3rem)",
					"slug": "heading-2"
				},
				{
					"name": "H1",
					"size": "clamp(3rem, 4.8vw, 4rem)",
					"slug": "heading-1"
				},
				{
					"name": "Display",
					"size": "clamp(3rem, 5.8vw, 5rem)",
					"slug": "display"
				}
			]
		},
		"useRootPaddingAwareAlignments": false
	},
	"styles": {
		"blocks": {
			"core/quote": {
				"variations": {
					"plain": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--font-quote)",
							"fontSize": "var(--wp--preset--font-size--heading-2)",
							"lineHeight": "var(--wp--custom--line-height--heading)",
							"fontStyle": "italic"
						},
						"spacing": {
							"margin": {
								"left": "0",
								"right": "0",
								"top": "var(--wp--custom--layout--gap)",
								"bottom": "var(--wp--custom--layout--gap)"
							},
							"padding": {
								"left": "0"
							}
						}
					}
				}
			},
			"core/media-text": {
				"spacing": {
					"blockGap": "var(--wp--custom--layout--gap-large)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--layout--gap)",
						"bottom": "var(--wp--custom--layout--gap)",
						"left": "var(--wp--custom--layout--gap)",
						"right": "var(--wp--custom--layout--gap)"
					}
				}
			},
			"core/navigation-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--neutral-100)",
			"text": "var(--wp--preset--color--white)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--05)",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--neutral-100)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)",
					"fontSize": "var(--wp--preset--font-size--body)",
					"fontWeight": "bold",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"fontWeight": "bold",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)",
					"fontWeight": "bold"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)",
					"fontWeight": "bold"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)",
					"fontWeight": "bold"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)",
					"fontWeight": "bold"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)",
					"lineHeight": "var(--wp--custom--line-height--headings)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)"
				}
			},
			"cite": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)", 
					"fontSize": "var(--wp--preset--font-size--body)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--gap)",
			"padding": {
				"bottom": "1rem",
				"left": "1rem",
				"right": "1rem",
				"top": "1rem"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--font-main)",
			"fontSize": "var(--wp--preset--font-size--body)",
			"lineHeight": "var(--wp--custom--line-height--normal)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "general",
			"name": "related-posts",
			"title": "Related Posts"
		}
	]
}